#include <iostream>
using namespace std;

int main() {
    int a, b, c, d;
    
    cin >> a >> b >> c >> d;
    
    int max_value = a;
    
    if (b > max_value) max_value = b;
    if (c > max_value) max_value = c;
    if (d > max_value) max_value = d;
    
    cout << max_value << endl;
    
    return 0;
}
